{
  "setupFiles": ["<rootDir>/src/setupTests.ts"],
  "testRegex": "/*.test.ts(x)",
  "collectCoverage": true,
  "coverageReporters": ["lcov"],
  "coverageDirectory": "coverage",
  "coverageThreshold": {
   "global": {
   "branches": 0,
   "functions": 0,
   "lines": 0,
   "statements": 0
   }
  },
  "moduleDirectories": ["node_modules", "src"]
 }
